{
    "layouts": {
        "main": {
            "type": "container",
            "layouts": [
                {
                    "type": "container",
                    "flow": "top-bottom",
                    "layouts": [
                        {
                            "type": "container",
                            "flow": "left-right",
                            "bind": "$.personal",
                            "layouts": [
                                {
                                    "type": "output",
                                    "bind": "@path",
                                    "component": {
                                        "name": "html"
                                    }
                                },
                                {
                                    "type": "output",
                                    "bind": "$root.html",
                                    "component": {
                                        "name": "html"
                                    }
                                }
                            ]
                        },
                        {
                            "type": "container",
                            "flow": "left-right",
                            "bind": "$.personal",
                            "layouts": [
                                {
                                    "type": "input",
                                    "bind": "$.firstName",
                                    "component": {
                                        "label": "First Name",
                                        "name": "text"
                                    }
                                },
                                {
                                    "type": "input",
                                    "bind": "$.firstName",
                                    "component": {
                                        "label": "First Name",
                                        "name": "text"
                                    }
                                },
                                {
                                    "type": "input",
                                    "bind": "$.lastName",
                                    "component": {
                                        "label": "Last Name",
                                        "name": "text"
                                    }
                                }
                            ]
                        },
                        {
                            "type": "container",
                            "flow": "top-bottom",
                            "layouts": [
                                {
                                    "type": "input",
                                    "bind": "$.personal.address",
                                    "component": {
                                        "label": "Address",
                                        "name": "text"
                                    }
                                }
                            ]
                        },
                        {
                            "type": "container",
                            "flow": "left-right",
                            "layouts": [
                                {
                                    "type": "input",
                                    "bind": "$.personal.city",
                                    "component": {
                                        "label": "City",
                                        "name": "text"
                                    }
                                },
                                {
                                    "type": "input",
                                    "bind": "$.personal.state",
                                    "component": {
                                        "label": "State",
                                        "name": "text"
                                    }
                                },
                                {
                                    "type": "input",
                                    "bind": "$.personal.zip",
                                    "component": {
                                        "label": "Zip",
                                        "name": "text"
                                    }
                                }
                            ]
                        },
                        {
                            "type": "output",
                            "bind": "@path",
                            "component": {
                                "name": "html"
                            }
                        },
                        {
                            "type": "list",
                            "bind": "$.contact",
                            "component": {
                                "name": "tabs",
                                "side": "left"
                            },
                            "layout": {
                                "type": "container",
                                "layouts": [
                                    {
                                        "type": "input",
                                        "bind": "@path",
                                        "component": {
                                            "label": "Index",
                                            "name": "text"
                                        }
                                    },
                                    {
                                        "type": "input",
                                        "bind": "$.type",
                                        "component": {
                                            "label": "Type",
                                            "name": "text"
                                        }
                                    },
                                    {
                                        "type": "input",
                                        "bind": "$.phoneNumber",
                                        "component": {
                                            "label": "Phone",
                                            "name": "text"
                                        }
                                    }
                                ]
                            }
                        }
                    ]
                }
            ]
        }
    }
}